<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:localPrimitives="using:Esri.ArcGISRuntime.Toolkit.Primitives"
    xmlns:local="using:Esri.ArcGISRuntime.Toolkit.UI.Controls"
    xmlns:internal="using:Esri.ArcGISRuntime.Toolkit.Internal">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="TimeSliderWhiteBrush" Color="White" />
            <SolidColorBrush x:Key="TimeSliderLightBrush" Color="#FFF0F0F0" />
            <SolidColorBrush x:Key="TimeSliderMediumBrush" Color="#FFE3E3E3" />
            <SolidColorBrush x:Key="TimeSliderMediumDarkBrush" Color="#FFA0A0A0" />
            <SolidColorBrush x:Key="TimeSliderDarkBrush" Color="#FF696969" />
            <SolidColorBrush x:Key="TimeSliderBlackBrush" Color="Black" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="TimeSliderWhiteBrush" Color="Black" />
            <SolidColorBrush x:Key="TimeSliderLightBrush" Color="#FF696969" />
            <SolidColorBrush x:Key="TimeSliderMediumBrush" Color="#FFA0A0A0" />
            <SolidColorBrush x:Key="TimeSliderMediumDarkBrush" Color="#FFE3E3E3" />
            <SolidColorBrush x:Key="TimeSliderDarkBrush" Color="#FFF0F0F0" />
            <SolidColorBrush x:Key="TimeSliderBlackBrush" Color="White" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="HighContrast">
            <SolidColorBrush x:Key="TimeSliderWhiteBrush" Color="White" />
            <SolidColorBrush x:Key="TimeSliderLightBrush" Color="#FFF0F0F0" />
            <SolidColorBrush x:Key="TimeSliderMediumBrush" Color="#FFE3E3E3" />
            <SolidColorBrush x:Key="TimeSliderMediumDarkBrush" Color="#FFA0A0A0" />
            <SolidColorBrush x:Key="TimeSliderDarkBrush" Color="#FF696969" />
            <SolidColorBrush x:Key="TimeSliderBlackBrush" Color="Black" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
    
    <ControlTemplate x:Key="StatelessButtonTemplate" TargetType="Button">
        <Border x:Name="border" BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                Background="{TemplateBinding Background}">
            <ContentPresenter x:Name="contentPresenter"
                              ContentTemplate="{TemplateBinding ContentTemplate}"
                              Content="{TemplateBinding Content}"
                              HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                              Margin="{TemplateBinding Padding}"
                              VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
        </Border>
    </ControlTemplate>

    <Style x:Key="TimeSliderThumbStyle" TargetType="Thumb">
        <Setter Property="Background" Value="{ThemeResource TimeSliderLightBrush}"/>
        <Setter Property="BorderBrush" Value="{ThemeResource TimeSliderBlackBrush}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Thumb">
                    <Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver" />
                                <VisualState x:Name="Pressed" />
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Duration="0"
                                                                       Storyboard.TargetName="NormalThumb"
                                                                       Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Duration="0"
                                                                       Storyboard.TargetName="PinnedThumb"
                                                                       Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Ellipse x:Name="NormalThumb"
                                 Fill="{TemplateBinding Background}"
                                 Stroke="{TemplateBinding BorderBrush}"
                                 StrokeThickness="1"/>
                        <Rectangle x:Name="PinnedThumb"
                                   Fill="{TemplateBinding Foreground}"
                                   Width="3"
                                   Visibility="Collapsed"
                                   Margin="0,4,0,4"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <Style TargetType="local:TimeSlider">
        <Setter Property="IsTabStop" Value="True" />
        <Setter Property="Foreground" Value="{ThemeResource TimeSliderBlackBrush}" />
        <Setter Property="Background" Value="{ThemeResource TimeSliderWhiteBrush}" />
        <Setter Property="BorderBrush" Value="{ThemeResource TimeSliderBlackBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="ThumbFill" Value="{ThemeResource TimeSliderLightBrush}"/>
        <Setter Property="ThumbStroke" Value="{ThemeResource TimeSliderBlackBrush}" />
        <Setter Property="CurrentExtentFill" Value="{ThemeResource TimeSliderDarkBrush}" />
        <Setter Property="FullExtentFill" Value="Transparent" />
        <Setter Property="FullExtentStroke" Value="{ThemeResource TimeSliderBlackBrush}" />
        <Setter Property="TimeStepIntervalTickFill" Value="{ThemeResource TimeSliderMediumDarkBrush}" />
        <Setter Property="PlaybackButtonsFill" Value="{ThemeResource TimeSliderMediumBrush}" />
        <Setter Property="PlaybackButtonsStroke" Value="{ThemeResource TimeSliderBlackBrush}" />
        <Setter Property="CurrentExtentLabelColor" Value="{ThemeResource TimeSliderBlackBrush}" />
        <Setter Property="FullExtentLabelColor" Value="{ThemeResource TimeSliderBlackBrush}" />
        <Setter Property="TimeStepIntervalLabelColor" Value="{ThemeResource TimeSliderBlackBrush}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:TimeSlider">

                    <Grid>
                        <Grid.Resources>
                            <internal:StringFormatConverter x:Key="StringFormatConverter" />

                            <Style x:Key="PlayPauseToggleButtonStyle" TargetType="ToggleButton">
                                <Setter Property="Padding" Value="0"/>
                                <Setter Property="BorderThickness" Value="0"/>

                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="ToggleButton">
                                            <Grid>
                                                <VisualStateManager.VisualStateGroups>
                                                    <VisualStateGroup x:Name="CommonStates">
                                                        <VisualState x:Name="MouseOver" />
                                                        <VisualState x:Name="Pressed" />
                                                        <VisualState x:Name="Disabled">
                                                            <Storyboard>
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="DisabledVisualElement" Storyboard.TargetProperty="Opacity">
                                                                    <SplineDoubleKeyFrame KeyTime="0" Value=".55"/>
                                                                </DoubleAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </VisualState>
                                                    </VisualStateGroup>
                                                    <VisualStateGroup x:Name="CheckStates">
                                                        <VisualState x:Name="Checked">
                                                            <Storyboard>
                                                                <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="PlayContent" Storyboard.TargetProperty="Visibility">
                                                                    <DiscreteObjectKeyFrame KeyTime="0">
                                                                        <DiscreteObjectKeyFrame.Value>
                                                                            <Visibility>Collapsed</Visibility>
                                                                        </DiscreteObjectKeyFrame.Value>
                                                                    </DiscreteObjectKeyFrame>
                                                                </ObjectAnimationUsingKeyFrames>
                                                                <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="PauseContent" Storyboard.TargetProperty="Visibility">
                                                                    <DiscreteObjectKeyFrame KeyTime="0">
                                                                        <DiscreteObjectKeyFrame.Value>
                                                                            <Visibility>Visible</Visibility>
                                                                        </DiscreteObjectKeyFrame.Value>
                                                                    </DiscreteObjectKeyFrame>
                                                                </ObjectAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </VisualState>
                                                        <VisualState x:Name="Normal">
                                                            <Storyboard>
                                                                <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="PlayContent" Storyboard.TargetProperty="Visibility">
                                                                    <DiscreteObjectKeyFrame KeyTime="0">
                                                                        <DiscreteObjectKeyFrame.Value>
                                                                            <Visibility>Visible</Visibility>
                                                                        </DiscreteObjectKeyFrame.Value>
                                                                    </DiscreteObjectKeyFrame>
                                                                </ObjectAnimationUsingKeyFrames>
                                                                <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="PauseContent" Storyboard.TargetProperty="Visibility">
                                                                    <DiscreteObjectKeyFrame KeyTime="0">
                                                                        <DiscreteObjectKeyFrame.Value>
                                                                            <Visibility>Collapsed</Visibility>
                                                                        </DiscreteObjectKeyFrame.Value>
                                                                    </DiscreteObjectKeyFrame>
                                                                </ObjectAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </VisualState>
                                                    </VisualStateGroup>
                                                    <VisualStateGroup x:Name="FocusStates">
                                                        <VisualState x:Name="Focused">
                                                            <Storyboard>
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="FocusVisualElement" Storyboard.TargetProperty="Opacity">
                                                                    <SplineDoubleKeyFrame KeyTime="0" Value="0.4"/>
                                                                </DoubleAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </VisualState>
                                                        <VisualState x:Name="Unfocused"/>
                                                    </VisualStateGroup>
                                                </VisualStateManager.VisualStateGroups>
                                                <Grid HorizontalAlignment="Center"
                                                      Margin="{TemplateBinding Padding}"
                                                      VerticalAlignment="Center" >
                                                    <Grid FlowDirection="LeftToRight"
                                                          Height="26"
                                                          Width="23">
                                                        <Grid x:Name="PlayContent">
                                                            <Path Data="M0,0 L0,26 22,13 0,0"
                                                                  Fill="{TemplateBinding Background}"
                                                                  Stroke="Transparent"/>
                                                            <Line X1="0" Y1="0" X2="22" Y2="13"
                                                                  Stroke="{TemplateBinding BorderBrush}"
                                                                  StrokeThickness="1" />
                                                            <Line X1="0" Y1="26" X2="22" Y2="13"
                                                                  Stroke="{TemplateBinding BorderBrush}"
                                                                  StrokeThickness="1" />
                                                            <Line X1="0" Y1="0" X2="0" Y2="26"
                                                                  Stroke="{TemplateBinding BorderBrush}"
                                                                  StrokeThickness="1" />
                                                        </Grid>
                                                        <Grid x:Name="PauseContent" Visibility="Collapsed">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="Auto" />
                                                                <ColumnDefinition Width="Auto" />
                                                            </Grid.ColumnDefinitions>
                                                            <Rectangle Width="8"
                                                                       StrokeThickness="1"
                                                                       HorizontalAlignment="Left"
                                                                       Fill="{TemplateBinding Background}"
                                                                       Stroke="{TemplateBinding BorderBrush}"
                                                                       VerticalAlignment="Stretch"/>
                                                            <Rectangle Width="8"
                                                                       StrokeThickness="1"
                                                                       Margin="3,0,0,0"
                                                                       HorizontalAlignment="Left"
                                                                       Fill="{TemplateBinding Background}"
                                                                       Stroke="{TemplateBinding BorderBrush}"
                                                                       VerticalAlignment="Stretch"
                                                                       Grid.Column="1"/>
                                                        </Grid>
                                                    </Grid>
                                                </Grid>
                                                <Rectangle x:Name="DisabledVisualElement" Fill="#FFFFFFFF" RadiusX="3" RadiusY="3" IsHitTestVisible="false" Opacity="0"/>
                                                <Rectangle x:Name="FocusVisualElement" Stroke="Transparent" StrokeThickness="1" RadiusX="2" RadiusY="2" Margin="1" IsHitTestVisible="false" Opacity="0"/>
                                            </Grid>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </Grid.Resources>

                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver"/>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="HorizontalTrackRectangleDisabledOverlay" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="MinimumThumbDisabledOverlay" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="MaximumThumbDisabledOverlay" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Focused">
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="FocusVisualElement" Storyboard.TargetProperty="Opacity">
                                            <SplineDoubleKeyFrame KeyTime="0" Value="0.4"/>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Unfocused"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>

                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>

                        <!-- start time label -->                        
                        <TextBlock x:Name="FullExtentStartTimeLabel"
                                   Text="{Binding FullExtent.StartTime,
                                        RelativeSource={RelativeSource TemplatedParent},
                                        Converter={StaticResource StringFormatConverter},
                                        ConverterParameter='{}{0:M/d/yyyy}'}"
                                   Foreground="{TemplateBinding FullExtentLabelColor}"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Bottom"
                                   Grid.Column="0"
                                   Grid.Row="0"
                                   Grid.ColumnSpan="2"
                                   Margin="0,0,0,1"/>

                        <!-- Playback buttons -->
                        <Grid Grid.Row="0"
                              Grid.Column="2"
                              Grid.RowSpan="2"
                              HorizontalAlignment="Center"
                              VerticalAlignment="Top"
                              Margin="0,0,0,2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <!-- Back button -->
                            <Button x:Name="PreviousButton"
                                    Grid.Column="0"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Right"
                                    BorderBrush="Transparent"
                                    Background="Transparent"
                                    Template="{StaticResource StatelessButtonTemplate}"
                                    Padding="0">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <Rectangle Width="1" 
                                               StrokeThickness="0"
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Stretch"
                                               Margin="0,0,1,1"
                                               Fill="{Binding PlaybackButtonsStroke, RelativeSource={RelativeSource TemplatedParent}}" />
                                    <Path Data="M12,0L12,14 0,7 12,0"
                                          Fill="{Binding PlaybackButtonsFill, RelativeSource={RelativeSource TemplatedParent}}"
                                          Stroke="{Binding PlaybackButtonsStroke, RelativeSource={RelativeSource TemplatedParent}}"
                                          StrokeThickness="1"
                                          Grid.Column="1"/>
                                </Grid>
                            </Button>

                            <!-- Play/pause button -->
                            <ToggleButton x:Name="PlayPauseButton"
                                          Grid.Column="1"
                                          Style="{StaticResource PlayPauseToggleButtonStyle}"
                                          VerticalAlignment="Bottom"
                                          Background="{TemplateBinding PlaybackButtonsFill}"
                                          BorderBrush="{TemplateBinding PlaybackButtonsStroke}"
                                          Margin="7,0,0,0"/>

                            <!-- Forward button -->
                            <Button x:Name="NextButton"
                                    Grid.Column="2" 
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Left"
                                    BorderBrush="Transparent"
                                    Background="Transparent"
                                    Margin="5,0,0,0"
                                    Padding="0"
                                    Template="{StaticResource StatelessButtonTemplate}">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <Path Data="M0,0L0,14 12,7 0,0"
                                          Fill="{Binding PlaybackButtonsFill, RelativeSource={RelativeSource TemplatedParent}}"
                                          Stroke="{Binding PlaybackButtonsStroke, RelativeSource={RelativeSource TemplatedParent}}"
                                          StrokeThickness="1"/>
                                    <Rectangle Width="1" 
                                               HorizontalAlignment="Left"
                                               VerticalAlignment="Stretch"
                                               StrokeThickness="0"
                                               Fill="{Binding PlaybackButtonsStroke, RelativeSource={RelativeSource TemplatedParent}}"
                                               Margin="1,0,0,1"
                                               Grid.Column="1"/>
                                </Grid>
                            </Button>
                        </Grid>

                        <!-- end time label -->                        
                        <TextBlock x:Name="FullExtentEndTimeLabel"
                                   Text="{Binding FullExtent.EndTime,
                                        RelativeSource={RelativeSource TemplatedParent},
                                        Converter={StaticResource StringFormatConverter},
                                        ConverterParameter='{}{0:M/d/yyyy}'}"
                                   Foreground="{TemplateBinding FullExtentLabelColor}"
                                   HorizontalAlignment="Right"
                                   VerticalAlignment="Bottom"
                                   Grid.Column="3"
                                   Grid.Row="0"
                                   Grid.ColumnSpan="2"
                                   Margin="0,0,0,1"/>

                        <!-- Slider track - controls to display manipulate current time extent -->
                        <RelativePanel x:Name="SliderTrack"
                              Grid.Row="2"
                              Grid.Column="1"
                              Grid.ColumnSpan="3"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Top">
                            
                            <!-- Start time tick mark -->
                            <Rectangle x:Name="StartTimeTickmark"
                                       Width="1"
                                       Height="6"
                                       Margin="0,0,0,0"
                                       Fill="{TemplateBinding FullExtentStroke}"/>

                            <!-- End time tick mark -->
                            <Rectangle x:Name="EndTimeTickmark"
                                       Width="1"
                                       Height="6"
                                       Margin="0,0,0,0"
                                       Fill="{TemplateBinding FullExtentStroke}"
                                       RelativePanel.AlignRightWithPanel="True"/>

                            <!-- Slider track focus element -->
                            <Rectangle x:Name="FocusVisualElement"
                                       Height="8"
                                       Stroke="{TemplateBinding FullExtentStroke}"
                                       StrokeThickness="1.5"
                                       IsHitTestVisible="false"
                                       Opacity="0"
                                       RelativePanel.Below="StartTimeTickmark"
                                       RelativePanel.AlignLeftWith="StartTimeTickmark"
                                       RelativePanel.AlignRightWith="EndTimeTickmark"/>

                            <!-- Slider track border -->
                            <Rectangle x:Name="SliderTrackBorder"
                                       Height="8"
                                       Stroke="{TemplateBinding FullExtentStroke}"
                                       Fill="{TemplateBinding FullExtentFill}"
                                       StrokeThickness="1"
                                       IsHitTestVisible="False"
                                       RelativePanel.Below="StartTimeTickmark"
                                       RelativePanel.AlignLeftWith="StartTimeTickmark"
                                       RelativePanel.AlignRightWith="EndTimeTickmark"/>

                            <!-- Repeater button to move time extent backward -->
                            <RepeatButton x:Name="SliderTrackStepBackRepeater"
                                          Height="{Binding Height, ElementName=SliderTrackBorder}"
                                          IsTabStop="False"
                                          Opacity="0"
                                          HorizontalAlignment="Stretch"
                                          RelativePanel.Below="StartTimeTickmark"/>

                            <!-- Repeater button to advance time extent forward -->
                            <RepeatButton x:Name="SliderTrackStepForwardRepeater"
                                          Height="{Binding Height, ElementName=SliderTrackBorder}"
                                          IsTabStop="False"
                                          Opacity="0"
                                          HorizontalAlignment="Stretch"
                                          RelativePanel.Below="StartTimeTickmark"/>

                            <!-- Middle thumb to drag time exten forward or backward -->
                            <Thumb x:Name="HorizontalTrackThumb"
                                   Height="{Binding Height, ElementName=SliderTrackBorder}"
                                   Foreground="{TemplateBinding CurrentExtentFill}"
                                   BorderBrush="{TemplateBinding FullExtentStroke}"
                                   BorderThickness="1"
                                   IsTabStop="False"
                                   RelativePanel.Below="StartTimeTickmark">
                                <Thumb.Template>
                                    <ControlTemplate>
                                        <Rectangle Fill="{TemplateBinding Foreground}"
                                                       Stroke="{TemplateBinding BorderBrush}"
                                                       StrokeThickness="{TemplateBinding BorderThickness}"/>
                                    </ControlTemplate>
                                </Thumb.Template>
                            </Thumb>

                            <!-- Middle thumb disabled state -->
                            <Border x:Name="HorizontalTrackRectangleDisabledOverlay"
                                    Height="{Binding Height, ElementName=SliderTrackBorder}"
                                    Background="Red"
                                    CornerRadius="5"
                                    Opacity="1"
                                    Visibility="Collapsed"
                                    HorizontalAlignment="Left"/>

                            <!-- Minimum thumb disabled state -->
                            <Rectangle x:Name="MinimumThumbDisabledOverlay"
                                       Width="10"
                                       Fill="{ThemeResource TimeSliderWhiteBrush}"
                                       RadiusX="2"
                                       RadiusY="2"
                                       Opacity=".55"
                                       Visibility="Collapsed"
                                       RelativePanel.AlignHorizontalCenterWith="MinimumThumb"
                                       RelativePanel.AlignVerticalCenterWith="MinimumThumb"/>

                            <!-- Maximum thumb disabled state -->
                            <Rectangle x:Name="MaximumThumbDisabledOverlay"
                                       Width="10"
                                       Height="Auto"
                                       Fill="{ThemeResource TimeSliderWhiteBrush}"
                                       RadiusX="2"
                                       RadiusY="2"
                                       Opacity=".55"
                                       Visibility="Collapsed"
                                       RelativePanel.AlignHorizontalCenterWith="MaximumThumb"
                                       RelativePanel.AlignVerticalCenterWith="MaximumThumb" />

                            <!-- Tick marks for displaying time step intervals -->
                            <localPrimitives:Tickbar x:Name="Tickmarks"
                                                     IsHitTestVisible="False"
                                                     TickFill="{TemplateBinding TimeStepIntervalTickFill}"
                                                     TickLabelColor="{TemplateBinding TimeStepIntervalLabelColor}"
                                                     RelativePanel.Below="SliderTrackBorder"
                                                     RelativePanel.AlignLeftWith="StartTimeTickmark"
                                                     RelativePanel.AlignRightWith="EndTimeTickmark">
                                <localPrimitives:Tickbar.MinorTickmarkTemplate>
                                    <DataTemplate>
                                        <Rectangle Width="1.5"
                                                   Height="4"
                                                   Fill="{Binding Parent.TickFill, RelativeSource={RelativeSource TemplatedParent}}" />
                                    </DataTemplate>
                                </localPrimitives:Tickbar.MinorTickmarkTemplate>
                                <localPrimitives:Tickbar.MajorTickmarkTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                            </Grid.RowDefinitions>
                                            <Rectangle Width="1.5"
                                                       Height="7"
                                                       Fill="{Binding Parent.TickFill, RelativeSource={RelativeSource TemplatedParent}}" />
                                            <TextBlock x:Name="TickLabel"
                                                       Text="{Binding Converter={StaticResource StringFormatConverter},
                                                                ConverterParameter='{}{0:M/d}'}"
                                                       Foreground="{Binding Parent.TickLabelColor, RelativeSource={RelativeSource TemplatedParent}}"
                                                       HorizontalAlignment="Center"
                                                       Grid.Row="1" />
                                        </Grid>
                                    </DataTemplate>
                                </localPrimitives:Tickbar.MajorTickmarkTemplate>
                            </localPrimitives:Tickbar>

                            <!-- Minimum thumb to control the start time -->
                            <Thumb x:Name="MinimumThumb"
                                   Width="12"
                                   Height="20"
                                   Style="{StaticResource TimeSliderThumbStyle}"
                                   BorderBrush="{TemplateBinding ThumbStroke}"
                                   Background="{TemplateBinding ThumbFill}"
                                   IsTabStop="False"
                                   RelativePanel.AlignVerticalCenterWith="SliderTrackBorder" />

                            <!--Mmaximum thumb to control the end time -->
                            <Thumb x:Name="MaximumThumb"
                                   Width="12"
                                   Height="20"
                                   Style="{StaticResource TimeSliderThumbStyle}"
                                   BorderBrush="{TemplateBinding ThumbStroke}"
                                   Background="{TemplateBinding ThumbFill}"
                                   IsTabStop="False"
                                   RelativePanel.AlignVerticalCenterWith="SliderTrackBorder" />

                            <!-- Start time label -->
                            <TextBlock x:Name="MinimumThumbLabel"
                                       Text="{Binding CurrentExtent.StartTime,
                                                RelativeSource={RelativeSource TemplatedParent},
                                                Converter={StaticResource StringFormatConverter},
                                                ConverterParameter='{}{0:M/d/yyyy}'}"
                                       Foreground="{TemplateBinding CurrentExtentLabelColor}"
                                       Visibility="Collapsed"
                                       RelativePanel.Below="MinimumThumb"/>

                            <TextBlock x:Name="MaximumThumbLabel"
                                       Text="{Binding CurrentExtent.EndTime,
                                                RelativeSource={RelativeSource TemplatedParent},
                                                Converter={StaticResource StringFormatConverter},
                                                ConverterParameter='{}{0:M/d/yyyy}'}"
                                       Foreground="{TemplateBinding CurrentExtentLabelColor}"
                                       Visibility="Collapsed"
                                       RelativePanel.Below="MaximumThumb"/>
                        </RelativePanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
