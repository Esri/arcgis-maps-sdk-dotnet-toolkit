            <BitmapImage x:Key="MeasureAreaImage" UriSource="ms-appx:///Esri.ArcGISRuntime.Toolkit.UWP/Assets/MeasureArea.png" />
            <BitmapImage x:Key="MeasureFeatureImage" UriSource="ms-appx:///Esri.ArcGISRuntime.Toolkit.UWP/Assets/MeasureFeature.png" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <BitmapImage x:Key="MeasureLengthImage" UriSource="ms-appx:///Esri.ArcGISRuntime.Toolkit.UWP/Assets/MeasureLengthDark.png" />
