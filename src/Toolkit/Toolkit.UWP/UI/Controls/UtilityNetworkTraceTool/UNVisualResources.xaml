    <Style x:Key="UNTracePathCommandIconStyle" TargetType="Path">
        <Setter Property="Width" Value="16" />
        <Setter Property="Height" Value="16" />
    </Style>

    <Style x:Key="UNTraceGroupBox" TargetType="internal:GroupBox">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="internal:GroupBox">
                    <Border
                        Padding="8"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <ContentPresenter Content="{TemplateBinding Header}" />
                            <ContentPresenter Grid.Row="1" Margin="0,8,0,0" />
                        </Grid>
                    </Border>

                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Foreground" Value="{StaticResource UNTraceForeground0}" />
    </Style>

    <Style x:Key="UNTraceComboBoxItem" TargetType="ComboBoxItem">
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="VerticalAlignment" Value="Stretch" />
        <Setter Property="Background" Value="{StaticResource UNTraceBackground1}" />
        <Setter Property="BorderBrush" Value="{StaticResource UNTraceBorder1}" />
        <Setter Property="BorderThickness" Value="1,0,1,1" />
        <Setter Property="Padding" Value="8" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ComboBoxItem">
                    <Border
                        Name="Border"
                        Margin="0"
                        Padding="{TemplateBinding Padding}"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}">
                        <ContentPresenter />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="UNTraceComboBoxToggleButton" TargetType="ToggleButton">
        <Setter Property="BorderBrush" Value="{StaticResource UNTraceBorder1}" />
        <Setter Property="Background" Value="{StaticResource UNTraceBackground1}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="7,7,7,6.5" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToggleButton">
                    <Grid x:Name="OuterGrid">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Border
                            Grid.ColumnSpan="2"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="0" />
                        <ContentPresenter
                            Grid.Column="0"
                            Margin="8,0,0,0"
                            Content="{TemplateBinding Content}" />
                        <Border
                            Grid.Column="1"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            BorderThickness="{TemplateBinding Padding}"
                            CornerRadius="0">
                            <Path
                                Grid.Column="1"
                                Width="12"
                                Height="12"
                                Margin="0,2,4,0"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Center"
                                Data="{StaticResource UNTraceIconCaretDown16}"
                                Stretch="Uniform"
                                Stroke="{StaticResource UNTraceForeground0}" />
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="UNTraceComboBoxTextBox" TargetType="TextBox">
        <Border
            Name="Border"
            Padding="{TemplateBinding Padding}"
            Background="{TemplateBinding Background}">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Border
                    x:Name="PART_ContentHost"
                    Grid.Column="1"
                    Margin="0"
                    Background="{TemplateBinding Background}" />
            </Grid>
        </Border>
    </ControlTemplate>
    <Style x:Key="UNTraceComboBox" TargetType="ComboBox">
        <Setter Property="Foreground" Value="{StaticResource UNTraceForeground0}" />
        <Setter Property="BorderBrush" Value="{StaticResource UNTraceBorder1}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Height" Value="28" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="Background" Value="{StaticResource UNTraceBackground1}" />
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto" />
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto" />
        <Setter Property="ItemContainerStyle" Value="{StaticResource UNTraceComboBoxItem}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ComboBox">
                    <Grid>
                        <ToggleButton
                            Name="ToggleButton"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            ClickMode="Press"
                            Foreground="{TemplateBinding Foreground}"
                            IsChecked="{Binding Path=IsDropDownOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                            Style="{StaticResource UNTraceComboBoxToggleButton}">
                            <ContentPresenter
                                Name="ContentSite"
                                Margin="0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Content="{TemplateBinding SelectionBoxItem}"
                                ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
                                ContentTemplateSelector="{TemplateBinding ItemTemplateSelector}"
                                IsHitTestVisible="False" />
                        </ToggleButton>
                        <TextBox
                            x:Name="PART_EditableTextBox"
                            Margin="4,0,32,0"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Center"
                            Background="{TemplateBinding Background}"
                            FontSize="12"
                            Foreground="{StaticResource UNTraceForeground0}"
                            Style="{x:Null}"
                            Template="{StaticResource UNTraceComboBoxTextBox}"
                            Visibility="Collapsed" />
                        <Popup Name="Popup">
                            <Grid
                                Name="DropDown"
                                MinWidth="{TemplateBinding ActualWidth}"
                                MaxHeight="{TemplateBinding MaxDropDownHeight}">
                                <Border
                                    x:Name="DropDownBorder"
                                    Background="{StaticResource UNTraceBackground0}"
                                    BorderBrush="{StaticResource UNTraceBorder1}"
                                    BorderThickness="1" />
                                <ScrollViewer>
                                    <StackPanel />
                                </ScrollViewer>
                            </Grid>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        x:Key="UNTraceTaggedComboBox"
        BasedOn="{StaticResource UNTraceComboBox}"
        TargetType="ComboBox">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ComboBox">
                    <Grid>
                        <ToggleButton
                            Name="ToggleButton"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            ClickMode="Press"
                            Foreground="{TemplateBinding Foreground}"
                            IsChecked="{Binding Path=IsDropDownOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                            Style="{StaticResource UNTraceComboBoxToggleButton}">
                            <ContentPresenter
                                Name="ContentSite"
                                Margin="0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Content="{TemplateBinding SelectionBoxItem}"
                                ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
                                ContentTemplateSelector="{TemplateBinding ItemTemplateSelector}"
                                IsHitTestVisible="False" />
                        </ToggleButton>
                        <TextBox
                            x:Name="PART_EditableTextBox"
                            Margin="4,0,32,0"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Center"
                            Background="{TemplateBinding Background}"
                            FontSize="12"
                            Foreground="{StaticResource UNTraceForeground0}"
                            Style="{x:Null}"
                            Template="{StaticResource UNTraceComboBoxTextBox}"
                            Visibility="Collapsed" />
                        <Popup Name="Popup" IsOpen="{TemplateBinding IsDropDownOpen}">
                            <Grid
                                Name="DropDown"
                                MinWidth="{TemplateBinding ActualWidth}"
                                MaxHeight="{TemplateBinding MaxDropDownHeight}">
                                <Border
                                    x:Name="DropDownBorder"
                                    Background="{StaticResource UNTraceBackground0}"
                                    BorderBrush="{StaticResource UNTraceBorder1}"
                                    BorderThickness="1" />
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <TextBlock
                                        Margin="8"
                                        FontWeight="SemiBold"
                                        Text="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Tag}"
                                        TextAlignment="Center" />
                                    <ScrollViewer Grid.Row="1">
                                        <StackPanel />
                                    </ScrollViewer>
                                </Grid>
                            </Grid>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="UNTraceExpanderToggleButtonTemplate" TargetType="ToggleButton">
        <Border
            x:Name="Border"
            Padding="{TemplateBinding Padding}"
            Background="{TemplateBinding Background}">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="4" />
                </Grid.ColumnDefinitions>

                <ContentPresenter />
                <Path
                    x:Name="CollapsedArrow"
                    Grid.Column="1"
                    Width="8"
                    Height="5.5"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Data="{StaticResource UNTraceIconChevronDown}"
                    Fill="{StaticResource UNTraceForeground0}"
                    Stretch="UniformToFill" />
                <Path
                    x:Name="ExpandededArrow"
                    Grid.Column="1"
                    Width="8"
                    Height="5.5"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Data="{StaticResource UNTraceIconChevronUp}"
                    Fill="{StaticResource UNTraceForeground0}"
                    Stretch="UniformToFill"
                    Visibility="Collapsed" />
            </Grid>
        </Border>
    </ControlTemplate>

    <Style x:Key="UNTraceExpander" TargetType="internal:Expander">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="{StaticResource UNTraceForeground0}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="Margin" Value="0,8,0,0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="internal:Expander">
                    <Grid x:Name="InnerGrid" Background="{TemplateBinding Background}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition x:Name="ContentRow" Height="Auto" />
                        </Grid.RowDefinitions>
                        <Border
                            x:Name="Border"
                            Padding="0"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
							<ToggleButton
                                Grid.Column="0"
                                MinHeight="0"
                                Margin="8,0,8,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                Background="Transparent"
                                Content="{TemplateBinding Header}"
                                FontSize="14"
                                FontWeight="SemiBold"
                                IsChecked="{TemplateBinding IsExpanded}" />
                        </Border>
                        <Border
                            x:Name="Content"
                            Grid.Row="1"
                            Margin="0,0,0,0"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            Visibility="{TemplateBinding IsExpanded}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                            <ContentPresenter Margin="{TemplateBinding Padding}" />
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="UNTraceTabItem" TargetType="PivotItem">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="PivotItem">
                    <Grid x:Name="Root">
                        <Border
                            x:Name="Border"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            BorderThickness="0,0,0,2">
                            <ContentPresenter
                                x:Name="ContentSite"
                                Margin="8"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Content="{TemplateBinding Header}" />
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="UNTracePrimaryButton" TargetType="ButtonBase">
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Padding" Value="4" />
        <Setter Property="MinHeight" Value="28" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ButtonBase">
                    <Border
                        x:Name="bg"
                        Padding="1"
                        Background="{StaticResource UNTraceTintNormal}"
                        BorderBrush="{StaticResource UNTraceTintNormal}"
                        BorderThickness="1">
                        <Grid>
                            <Grid HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <ContentPresenter
                                    x:Name="content"
                                    Grid.Column="1"
                                    Content="{TemplateBinding Content}" />
                            </Grid>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="UNTraceIconButton" TargetType="Button">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border
                        x:Name="bg"
                        Margin="0"
                        Padding="0"
                        Background="Transparent"
                        BorderThickness="0">
                        <Grid HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <ContentPresenter
                                x:Name="content"
                                Grid.Column="1"
                                Content="{TemplateBinding Content}" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        x:Key="UNTraceSecondaryButton"
        BasedOn="{StaticResource UNTracePrimaryButton}"
        TargetType="ButtonBase">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ButtonBase">
                    <Border
                        x:Name="bg"
                        Padding="1"
                        Background="{StaticResource UNTraceBackground0}"
                        BorderBrush="{StaticResource UNTraceTintNormal}"
                        BorderThickness="1">
                        <Grid>
                            <Grid HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <ContentPresenter
                                    x:Name="content"
                                    Grid.Column="1"
                                    Content="{TemplateBinding Content}" />
                            </Grid>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="UNTraceTabControl" TargetType="Pivot">
        <Setter Property="ItemContainerStyle" Value="{StaticResource UNTraceTabItem}" />
    </Style>

    <Style x:Key="UNTraceTextBox" TargetType="TextBox">
        <Setter Property="Background" Value="{StaticResource UNTraceBackground1}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="FontSize" Value="12" />
        <Setter Property="Padding" Value="4" />
        <Setter Property="MinWidth" Value="64" />
        <Setter Property="MinHeight" Value="28" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="AcceptsReturn" Value="True" />
        <Setter Property="TextWrapping" Value="Wrap" />
        <Setter Property="BorderBrush" Value="{StaticResource UNTraceBorder1}" />
        <Setter Property="Foreground" Value="{StaticResource UNTraceForeground0}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="TextBox">
                    <Grid>
                        <Border
                            Name="Border"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <ScrollViewer
                                    x:Name="PART_ContentHost"
                                    Grid.Column="1"
                                    Margin="0"
                                    Padding="0" />
                            </Grid>
                        </Border>
                        <Border
                            x:Name="PART_Scrim"
                            Background="{TemplateBinding Background}"
                            Opacity="0.5"
                            Visibility="Collapsed" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        x:Key="UNTraceTextBoxCompact"
        BasedOn="{StaticResource UNTraceTextBox}"
        TargetType="TextBox">
        <Setter Property="FontSize" Value="11" />
        <Setter Property="Padding" Value="2" />
        <Setter Property="MinHeight" Value="0" />
        <Setter Property="MinWidth" Value="0" />
    </Style>

    <Style x:Key="UNTraceLabelingGroupBox" TargetType="internal:GroupBox">
        <Setter Property="Margin" Value="0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="internal:GroupBox">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Border
                            x:Name="bd_header"
                            Grid.Row="0"
                            Margin="0,0,0,4"
                            Background="Transparent"
                            BorderThickness="0">
                            <ContentPresenter Content="{TemplateBinding Header}" />
                        </Border>
                        <Border x:Name="bd_body" Grid.Row="1">
                            <ContentPresenter />
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="UNTraceListViewItem" TargetType="ListViewItem">
        <Setter Property="Padding" Value="8" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="{StaticResource UNTraceBorder1}" />
        <Setter Property="BorderThickness" Value="0,0,0,1" />
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="Foreground" Value="{StaticResource UNTraceForeground0}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Item">
                    <Border
                        x:Name="Bd"
                        Padding="{TemplateBinding Padding}"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}">
                        <ContentPresenter
                            x:Name="ContentPart"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="UNTraceListView" TargetType="ListView">
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="Background" Value="{StaticResource UNTraceBackground0}" />
        <Setter Property="BorderBrush" Value="{StaticResource UNTraceBorder1}" />
        <Setter Property="ItemContainerStyle" Value="{StaticResource UNTraceListViewItem}" />
    </Style>

    <Style x:Key="UNTraceCheckBox" TargetType="CheckBox">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="CheckBox">
                    <ContentPresenter
                        x:Name="ContentPresenterPart"
                        Margin="6,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="UNTraceCheckBoxEye" TargetType="CheckBox">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="CheckBox">
                    <ContentPresenter
                        x:Name="ContentPresenterPart"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="UNTraceProgressBar" TargetType="ProgressBar">
        <Setter Property="Height" Value="2" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="IsIndeterminate" Value="True" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="Margin" Value="0,8,0,8" />
        <Setter Property="Foreground" Value="{StaticResource UNTraceTintNormal}" />
        <Setter Property="Background" Value="{StaticResource UNTraceBorder1}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ProgressBar">
                    <Grid Background="{TemplateBinding Background}">
                        <Border
                            x:Name="PART_Track"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}" />
                        <Border
                            x:Name="PART_Indicator"
                            HorizontalAlignment="Left"
                            BorderThickness="0">
                            <Grid x:Name="Animation">
                                <Border
                                    x:Name="ProgressFill"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Stretch"
                                    Background="{TemplateBinding Foreground}" />
                                <Rectangle
                                    x:Name="PART_GlowRect"
                                    Width="100"
                                    Margin="-100,0,0,0"
                                    HorizontalAlignment="Left"
                                    Fill="{TemplateBinding Foreground}"
                                    Stroke="{TemplateBinding Foreground}" />
                            </Grid>
                        </Border>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Determinate" />
                                <VisualState x:Name="Indeterminate">
                                    <Storyboard>
                                        <DoubleAnimation
                                            Storyboard.TargetName="ProgressFill"
                                            Storyboard.TargetProperty="Opacity"
                                            To="0"
                                            Duration="00:00:00" />
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="UNTraceSliderButtonStyle" TargetType="RepeatButton">
        <Setter Property="IsTabStop" Value="false" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="RepeatButton">
                    <Border Background="Transparent" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="UNTraceSlider" TargetType="Slider">
        <Setter Property="LargeChange" Value="0.1" />
        <Setter Property="Maximum" Value="1.0" />
        <Setter Property="Minimum" Value="0.0" />
        <Setter Property="SmallChange" Value="0.05" />
        <Setter Property="TickFrequency" Value="0.1" />
        <Setter Property="TickPlacement" Value="BottomRight" />
        <Setter Property="Foreground" Value="{StaticResource UNTraceTintNormal}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Slider">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" MinHeight="{TemplateBinding MinHeight}" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>

                        <Border
                            x:Name="TrackBackground"
                            Grid.Row="1"
                            Height="2"
                            Margin="0"
                            Background="{StaticResource UNTraceBorder1}"
                            BorderThickness="0" />
                        <ProgressBar
                            x:Name="InnerProgress"
                            Grid.Row="1"
                            Height="2"
                            Margin="0"
                            Background="Transparent"
                            BorderThickness="0"
                            Foreground="{TemplateBinding Foreground}"
                            Maximum="{TemplateBinding Maximum}"
                            Minimum="{TemplateBinding Minimum}"
                            Value="{TemplateBinding Value}" />
                        <TickBar
                            x:Name="TopTick"
                            Grid.Row="1"
                            Height="4"
                            VerticalAlignment="Center"
                            Fill="{StaticResource UNTraceForeground3}"
                            Visibility="Collapsed" />
                        <!--<Track x:Name="PART_Track" Grid.Row="1">
                            <Track.Thumb>
                                <Thumb>
                                    <Thumb.Template>
                                        <ControlTemplate TargetType="Thumb">
                                            <Ellipse
                                                x:Name="Ellipse"
                                                Width="10"
                                                Height="10"
                                                Fill="{StaticResource UNTraceBackground0}"
                                                Stroke="{StaticResource UNTraceForeground3}"
                                                StrokeThickness="2" />
                                        </ControlTemplate>
                                    </Thumb.Template>
                                </Thumb>
                            </Track.Thumb>
                        </Track>-->
                        <TickBar
                            x:Name="BottomTick"
                            Grid.Row="2"
                            Height="4"
                            Fill="{StaticResource UNTraceForeground3}"
                            Visibility="Collapsed" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="UNTraceHighCapacityTabControl" TargetType="Pivot">
        <Setter Property="ItemContainerStyle" Value="{StaticResource UNTraceTabItem}" />
        <Setter Property="Pivot.RightHeader">
            <Setter.Value>

                <Grid>

                    <ToggleButton
                        x:Name="MenuToggle"
                        Grid.Column="1"
                        Width="32"
                        MinHeight="32"
                        VerticalAlignment="Stretch"
                        BorderBrush="Transparent"
                        Style="{StaticResource UNTraceSecondaryButton}"
                        Visibility="{Binding ElementName=HeaderPanel, Path=ComputedHorizontalScrollBarVisibility, Mode=OneWay}">
                        <Path
                            Width="16"
                            Height="16"
                            Data="{StaticResource UNTraceIconFormDropdown}"
                            Fill="{StaticResource UNTraceTintNormal}"
                            Style="{StaticResource UNTracePathCommandIconStyle}" />
                    </ToggleButton>
                    <Popup
                        Width="240"
                        Height="240"
                        IsOpen="{Binding ElementName=MenuToggle, Path=IsChecked, Mode=TwoWay}">
                        <ListView
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            ItemContainerStyle="{StaticResource UNTraceListViewItem}"
                            ItemsSource="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=ItemsSource}"
                            Style="{StaticResource UNTraceListView}">
                            <ListView.ItemTemplate>
                                <DataTemplate>
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <TextBlock
                                            MaxWidth="136"
                                            Margin="0,0,8,0"
                                            VerticalAlignment="Center"
                                            Text="{Binding Name}"
                                            TextWrapping="Wrap" />
                                        <Button
                                            Grid.Column="1"
                                            Margin="0,0,8,0"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Center"
                                            CommandParameter="{Binding}"
                                            Style="{StaticResource UNTraceIconButton}">
                                            <Path Data="{StaticResource UNTraceIconZoomTo}" Style="{StaticResource UNTracePathCommandIconStyle}" />
                                        </Button>
                                        <CheckBox
                                            Grid.Column="2"
                                            Margin="0,0,8,0"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Center"
                                            IsChecked="{Binding VisualizationState, Mode=TwoWay}"
                                            Style="{StaticResource UNTraceCheckBoxEye}" />
                                        <Button
                                            Grid.Column="3"
                                            Margin="0,0,0,0"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Center"
                                            CommandParameter="{Binding}"
                                            Style="{StaticResource UNTraceIconButton}">
                                            <Path Data="{StaticResource UNTraceIconDelete}" Style="{StaticResource UNTracePathCommandIconStyle}" />
                                        </Button>
                                    </Grid>
                                </DataTemplate>
                            </ListView.ItemTemplate>
                        </ListView>
                    </Popup>
                </Grid>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>