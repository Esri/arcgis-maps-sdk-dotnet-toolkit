        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="FocusVisualStyle" Value="{StaticResource UNTraceFocusVisual}" />
        <Setter Property="Foreground" Value="{StaticResource UNTraceForeground0}" />
        <Setter Property="Template">
