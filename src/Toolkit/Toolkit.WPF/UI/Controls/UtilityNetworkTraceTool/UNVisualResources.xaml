        <Setter Property="FontSize" Value="12" />
        <Setter Property="Padding" Value="4" />
        <Setter Property="MinWidth" Value="64" />
        <Setter Property="MinHeight" Value="28" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="AcceptsReturn" Value="True" />
        <Setter Property="Cursor" Value="IBeam" />
        <Setter Property="TextWrapping" Value="Wrap" />
        <Setter Property="FocusVisualStyle" Value="{StaticResource UNTraceFocusVisual}" />
        <Setter Property="CaretBrush" Value="{StaticResource UNTraceTintNormal}" />
        <Setter Property="SelectionBrush" Value="{StaticResource UNTraceTintNormal}" />
        <Setter Property="BorderBrush" Value="{StaticResource UNTraceBorder1}" />
        <Setter Property="Foreground" Value="{StaticResource UNTraceForeground0}" />
        <Setter Property="SelectionBrush" Value="{StaticResource UNTraceTintNormal}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBoxBase}">
                    <Grid>
                        <Border
                            Name="Border"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <ScrollViewer
                                    x:Name="PART_ContentHost"
                                    Grid.Column="1"
                                    Margin="0"
                                    Padding="0" />
                            </Grid>
                        </Border>
                        <Border
                            x:Name="PART_Scrim"
                            Background="{TemplateBinding Background}"
                            Opacity="0.5"
                            Visibility="Collapsed" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="PART_Scrim" Property="Visibility" Value="Visible" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsFocused" Value="True" />
                    <Condition Property="IsReadOnly" Value="False" />
                </MultiTrigger.Conditions>
                <MultiTrigger.Setters>
                    <Setter Property="BorderBrush" Value="{StaticResource UNTraceTintNormal}" />
                </MultiTrigger.Setters>
            </MultiTrigger>
            <Trigger Property="IsReadOnly" Value="True">
                <Setter Property="Background" Value="{StaticResource UNTraceBackground2}" />
            </Trigger>
        </Style.Triggers>
