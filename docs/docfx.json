{
  "metadata": [    
    {
      "src": [
        {
          "files": [ 
            "Toolkit/Toolkit.WPF/Esri.ArcGISRuntime.Toolkit.WPF.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src" : "../src/"
        }
      ],
      "dest": "../Output/docs/api/netfx",
      "properties": { "TargetFramework": "net472" },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },    
    {
      "src": [
        {
          "files": [
            "Toolkit/Toolkit.WPF/Esri.ArcGISRuntime.Toolkit.WPF.csproj",
            "Toolkit/Toolkit.WinUI/Esri.ArcGISRuntime.Toolkit.WinUI.csproj",
            "Toolkit/Toolkit.Maui/Esri.ArcGISRuntime.Toolkit.Maui.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src" : "../src/"
        }
      ],
      "dest": "../Output/docs/api/net6",
      "properties": { "TargetFramework": "net9.0-windows10.0.19041.0" },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "Toolkit/Toolkit.Maui/Esri.ArcGISRuntime.Toolkit.Maui.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src" : "../src/"
        }
      ],
      "dest": "../Output/docs/api/android",
      "properties": { "TargetFramework": "net9.0-android" },
      "filter": "filterConfig.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "Toolkit/Toolkit.Maui/Esri.ArcGISRuntime.Toolkit.Maui.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src" : "../src/"
        }
      ],
      "dest": "../Output/docs/api/ios",
      "properties": { "TargetFramework": "net9.0-ios" },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "Toolkit/Toolkit.UWP/Esri.ArcGISRuntime.Toolkit.UWP.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src" : "../src/"
        }
      ],
      "dest": "../Output/docs/api/uwp",
      "properties": { "TargetFramework": "uap10.0.19041" },
      "filter": "filterConfig.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**/index.md",
          "api/index.md"
        ]
      },
      {
        "files": [
          "api/**.yml",
          "api/**/toc.yml"
        ],
        "src" : "../Output/docs/"
      },
      {
        "files": [
          "concepts/**.md",
          "concepts/**.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ]
      }
    ],
    "globalMetadata": {
      "_appLogoPath": "images/logo.png",
      "_gitContribute": {
        "branch": "main",
        "apiSpecFolder": "docs/api"
      }
    },
    "dest": "../Output/docs_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default", "template-override"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
